<div id="map-canvas"></div>
<div class="sidebar">
  <form action="/maps/search" class="form" id="search">
    <div class="form-group">
      <label for="start-address">Start Address:</label>
      <input type="text" name="start-address" class="form-control" id="start-address" required>
    </div>
    <div class="form-group">
      <label for="end-address">End Address:</label>
      <input type="text" name="end-address" class="form-control" id="end-address" required>
    </div>
    <button class="btn btn-primary" type="submit">Search</button>
  </form>
</div>